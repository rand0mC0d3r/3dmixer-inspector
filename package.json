{
  "name": "aframe-touch-inspector",
  "version": "1.0.3",
  "homepage": "https://rand0mc0d3r.github.io/aframe-touch-inspector/examples/",
  "description": "A touch of a visual inspector tool inspired by A-Frame-Inspector for A-Frame",
  "main": "dist/aframe-inspector.min.js",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "postinstall": "npm dedupe",
    "build": "cross-env NODE_ENV=production webpack --progress --colors",
    "dist": "npm run dist:max && npm run dist:min",
    "dist:analyze": "npm run dev -- --env.addons=bundleanalyzer",
    "copy": "npm run dist:max && npm run dist:min",
    "dist:max": "cross-env AFRAME_DIST=true npm run build",
    "dist:min": "cross-env MINIFY=true AFRAME_DIST=true npm run build",
    "postdist": "cp ./dist/*.min* ../alumob/public/",
    "start": "cross-env NODE_ENV=dev webpack-dev-server --progress --colors -d --host 0.0.0.0",
    "explorer": "source-map-explorer ./dist/*.js",
    "unused": "npm-check ./",
    "stats": " source-map-explorer ./dist/*.js --json"
  },
  "repository": "rand0mC0d3r/aframe-touch-inspector",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@material-ui/core": "^4.11.0",
    "lodash.debounce": "4.0.8",
    "classnames": "^2.2.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.13.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-unused-imports": "^1.0.0",
    "npm-check": "^5.9.2",
    "npm-force-resolutions": "0.0.3",
    "source-map-explorer": "^2.5.0",
    "style-loader": "^2.0.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "resolutions": {
    "serialize-javascript": "^5.0.1"
  }
}
